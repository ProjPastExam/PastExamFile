// Script assets have changed for v2.3.0 see
// https://help.yoyogames.com/hc/en-us/articles/360005277377 for more information
function sc_keySprite(keyIndex){
	switch (keyIndex) {
	case vk_up:		sprite_index = sp_keyVk_up;		break;
	case vk_down:	sprite_index = sp_keyVk_down;	break;
	case vk_left:	sprite_index = sp_keyVk_left;	break;
	case vk_right:	sprite_index = sp_keyVk_right;	break;
	case vk_escape:	sprite_index = sp_keyVk_esc;	break;
	case vk_space:	sprite_index = sp_keyVk_space;	break;
	case vk_tab:	sprite_index = sp_keyVk_tap;	break;
	case vk_shift:	sprite_index = sp_keyVk_shift;	break;
	case ord("0"):	sprite_index = sp_key_0;		break;
	case ord("1"):	sprite_index = sp_key_1;		break;
	case ord("2"):	sprite_index = sp_key_2;		break;
	case ord("3"):	sprite_index = sp_key_3;		break;
	case ord("4"):	sprite_index = sp_key_4;		break;
	case ord("5"):	sprite_index = sp_key_5;		break;
	case ord("6"):	sprite_index = sp_key_6;		break;
	case ord("7"):	sprite_index = sp_key_7;		break;
	case ord("8"):	sprite_index = sp_key_8;		break;
	case ord("9"):	sprite_index = sp_key_9;		break;
	case ord("A"):	sprite_index = sp_key_a;		break;
	case ord("B"):	sprite_index = sp_key_b;		break;
	case ord("C"):	sprite_index = sp_key_c;		break;
	case ord("D"):	sprite_index = sp_key_d;		break;
	case ord("E"):	sprite_index = sp_key_e;		break;
	case ord("F"):	sprite_index = sp_key_f;		break;
	case ord("G"):	sprite_index = sp_key_g;		break;
	case ord("H"):	sprite_index = sp_key_h;		break;
	case ord("I"):	sprite_index = sp_key_i;		break;
	case ord("J"):	sprite_index = sp_key_j;		break;
	case ord("K"):	sprite_index = sp_key_k;		break;
	case ord("L"):	sprite_index = sp_key_l;		break;
	case ord("M"):	sprite_index = sp_key_m;		break;
	case ord("N"):	sprite_index = sp_key_n;		break;
	case ord("O"):	sprite_index = sp_key_o;		break;
	case ord("P"):	sprite_index = sp_key_p;		break;
	case ord("S"):	sprite_index = sp_key_s;		break;
	case ord("T"):	sprite_index = sp_key_t;		break;
	case ord("U"):	sprite_index = sp_key_u;		break;
	case ord("V"):	sprite_index = sp_key_v;		break;
	case ord("W"):	sprite_index = sp_key_w;		break;
	case ord("X"):	sprite_index = sp_key_x;		break;
	case ord("Y"):	sprite_index = sp_key_y;		break;
	case ord("Z"):	sprite_index = sp_key_z;		break;
	}
}